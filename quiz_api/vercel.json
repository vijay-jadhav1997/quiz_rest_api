{
  "version": 2,
  "builds": [{
      "src": "quiz_api/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static_build",
      "config": {"distDir": "staticfiles_build"}
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "quiz_api/wsgi.py",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}